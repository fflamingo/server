{
  "compilerOptions": {
    "target": "es6",
    "jsx": "react",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": ["es2015", "dom"],
    "declaration": true,
    "sourceMap": true,
    "outDir": "lib",
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "strict": true,
    "esModuleInterop": true
  }
}
